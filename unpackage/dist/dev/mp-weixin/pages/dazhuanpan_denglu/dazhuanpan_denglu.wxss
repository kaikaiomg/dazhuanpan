
.logo {
  width: 750px;
  height: 240px;
  padding: 20px;
  box-sizing: border-box;
  background-color: #FF80AB;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.logo-hover {
  opacity: 0.8;
}